<keymap version="1" name="Default copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="shift ctrl q" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="ExpandAllToLevel1">
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="1" />
    <keyboard-shortcut first-keystroke="shift ctrl multiply" second-keystroke="numpad1" />
    <keyboard-shortcut first-keystroke="shift ctrl comma" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="ctrl alt w" />
  </action>
</keymap>